{
    "name": "sbuerk/tf8-test-extension",
    "description": "typo3/testing-framework v8 extension level test",
    "license": "GPL-2.0-or-later",
    "type": "typo3-cms-extension",
    "authors": [
        {
            "name": "Stefan BÃ¼rk",
            "email": "stefan@buerk.tech",
            "role": "Maintainer"
        }
    ],
    "homepage": "https://github.com/sbuerk/tf8-test-extension",
    "support": {
        "issues": "https://github.com/sbuerk/tf8-test-extension/issues"
    },
    "require": {
        "php": "^8.1 || ^8.2"
    },
    "require-dev": {
        "bnf/phpstan-psr-container": "^1.0.1",
        "codeception/codeception": "^5.0.10",
        "codeception/lib-asserts": "^2.1.0",
        "codeception/module-asserts": "^3.0.0",
        "codeception/module-cli": "^2.0.1",
        "codeception/module-filesystem": "^3.0.0",
        "codeception/module-webdriver": "^4.0.0",
        "ergebnis/composer-normalize": "^2.30.2",
        "friendsofphp/php-cs-fixer": "^3.16.0",
        "friendsoftypo3/phpstan-typo3": "^0.9.0",
        "phpstan/phpstan": "^1.10.14",
        "phpstan/phpstan-phpunit": "^1.3.11",
        "phpunit/phpunit": "^10.1",
        "typo3/cms-adminpanel": "~12.4@dev || ~13.0@dev",
        "typo3/cms-backend": "~12.4@dev || ~13.0@dev",
        "typo3/cms-belog": "~12.4@dev || ~13.0@dev",
        "typo3/cms-beuser": "~12.4@dev || ~13.0@dev",
        "typo3/cms-core": "~12.4@dev || ~13.0@dev",
        "typo3/cms-dashboard": "~12.4@dev || ~13.0@dev",
        "typo3/cms-extbase": "~12.4@dev || ~13.0@dev",
        "typo3/cms-extensionmanager": "~12.4@dev || ~13.0@dev",
        "typo3/cms-felogin": "~12.4@dev || ~13.0@dev",
        "typo3/cms-filelist": "~12.4@dev || ~13.0@dev",
        "typo3/cms-fluid": "~12.4@dev || ~13.0@dev",
        "typo3/cms-fluid-styled-content": "~12.4@dev || ~13.0@dev",
        "typo3/cms-form": "~12.4@dev || ~13.0@dev",
        "typo3/cms-frontend": "~12.4@dev || ~13.0@dev",
        "typo3/cms-indexed-search": "~12.4@dev || ~13.0@dev",
        "typo3/cms-info": "~12.4@dev || ~13.0@dev",
        "typo3/cms-install": "~12.4@dev || ~13.0@dev",
        "typo3/cms-lowlevel": "~12.4@dev || ~13.0@dev",
        "typo3/cms-recycler": "~12.4@dev || ~13.0@dev",
        "typo3/cms-reports": "~12.4@dev || ~13.0@dev",
        "typo3/cms-scheduler": "~12.4@dev || ~13.0@dev",
        "typo3/cms-seo": "~12.4@dev || ~13.0@dev",
        "typo3/cms-setup": "~12.4@dev || ~13.0@dev",
        "typo3/cms-sys-note": "~12.4@dev || ~13.0@dev",
        "typo3/cms-tstemplate": "~12.4@dev || ~13.0@dev",
        "typo3/cms-workspaces": "~12.4@dev || ~13.0@dev",
        "typo3/testing-framework": "dev-main"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "SBUERK\\TF8TestExtension\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SBUERK\\TF8TestExtension\\Tests\\": "Tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        },
        "bin-dir": ".Build/bin",
        "preferred-install": {
            "typo3/testing-framework": "source",
            "*": "dist"
        },
        "sort-packages": true,
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x.x-dev"
        },
        "typo3/cms": {
            "app-dir": ".Build",
            "extension-key": "tf8_extension",
            "web-dir": ".Build/Web"
        }
    }
}
